{
    "name": "rails",
    "displayName": "rails",
    "description": "Ruby on Rails \"Asset Helpers\" and \"Tag Helpers\" snippets. .erb syntax highlights,related files Navigation,Go to Definition",
    "version": "0.5.7",
    "publisher": "bung87",
    "icon": "rails.png",
    "engines": {
        "vscode": "^1.19.3"
    },
    "galleryBanner": {
        "color": "#c52f24",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bung87/vscode-rails.git"
    },
    "homepage": "https://github.com/bung87/vscode-rails",
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://github.com/bung87/vscode-rails/issues"
    },
    "categories": [
        "Languages",
        "Snippets",
        "Other"
    ],
    "keywords": [
        "rails",
        "ROR",
        "Ruby on Rails",
        ".erb"
    ],
    "activationEvents": [
        "onCommand:rails-nav",
        "onLanguage:ruby"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "rails-nav",
                "title": "Rails Nav"
            }
        ],
        "keybindings": [
            {
                "command": "rails-nav",
                "key": "Alt+."
            }
        ],
        "snippets": [
            {
                "language": "js.erb",
                "path": "./snippets/js.erb.json"
            },
            {
                "language": "html.erb",
                "path": "./snippets/html.erb.json"
            },
            {
                "language": "css.erb",
                "path": "./snippets/css.erb.json"
            },
            {
                "language": "scss.erb",
                "path": "./snippets/scss.erb.json"
            },
            {
                "language": "ruby",
                "path": "./snippets/ruby.json"
            }
        ],
        "languages": [
            {
                "id": "html.erb",
                "aliases": [
                    "html.erb"
                ],
                "extensions": [
                    ".html.erb",
                    ".rhtml",
                    ".rhtm"
                ]
            },
            {
                "id": "js.erb",
                "aliases": [
                    "js.erb"
                ],
                "extensions": [
                    ".js.erb"
                ]
            },
            {
                "id": "css.erb",
                "aliases": [
                    "css.erb"
                ],
                "extensions": [
                    ".css.erb"
                ]
            },
            {
                "id": "scss.erb",
                "extensions": [
                    ".scss.erb"
                ]
            }
        ],
        "grammars": [
            {
                "language": "html.erb",
                "scopeName": "text.html.erb",
                "path": "./syntaxes/html.erb.tmLanguage"
            },
            {
                "language": "js.erb",
                "scopeName": "source.js.erb",
                "path": "./syntaxes/js.erb.tmLanguage"
            },
            {
                "language": "scss.erb",
                "scopeName": "source.scss",
                "path": "./syntaxes/scss.tmLanguage"
            },
            {
                "language": "css.erb",
                "scopeName": "source.scss",
                "path": "./syntaxes/scss.tmLanguage"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "glob": "^7.1.2",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.1.18"
    },
    "dependencies": {
        "inflection": "^1.12.0",
        "inherits": "^2.0.3",
        "minimatch": "^3.0.4",
        "minimist": "0.0.8",
        "n-readlines": "^0.2.8",
        "npm": "^5.7.1"
    }
}
