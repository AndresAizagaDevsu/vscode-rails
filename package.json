{
    "name": "rails",
    "displayName": "rails",
    "description": "Ruby on Rails \"Asset Tag Helpers\" and \"Form Helpers snippets.",
    "version": "0.0.4",
    "publisher": "bung87",
    "icon": "rails.png",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bung87/vscode-rails.git"
    },
    "homepage": "https://github.com/bung87/vscode-rails",
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://github.com/bung87/vscode-rails/issues"
    },
    "categories": [
        "Languages",
        "Snippets",
        "Other"
    ],
    "keywords": [
        "rails",
        "ROR",
        "Ruby on Rails",
        ".erb"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "html",
                "path": "./snippets/html.json"
            },
            {
                "language": "css",
                "path": "./snippets/css.json"
            },
             {
                "language": "scss",
                "path": "./snippets/scss.json"
            }
        ],
        "languages": [
            {
                "id": "html",
                "aliases": [
                    "html.erb"
                ],
                "extensions": [
                    ".html.erb"
                ]
            },
            {
                "id": "javascript",
                "aliases": [
                    "js.erb"
                ],
                "extensions": [
                    ".js.erb"
                ]
            },
            {
                "id": "css",
                "aliases": [
                    "css.erb"
                ],
                "extensions": [
                    ".css.erb"
                ]
            },
            {
                "id": "scss",
                "extensions": [
                    ".scss"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}